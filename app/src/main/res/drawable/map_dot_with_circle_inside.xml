<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="20dp"
                android:left="20dp"
                android:right="20dp"
                android:top="20dp" />
            <size
                android:width="100dp"
                android:height="100dp" />
            <stroke
                android:width="20dp"
                android:color="@color/aircasting_blue_400" />
        </shape>
    </item>

    <item android:drawable="@drawable/ic_dot_20" />

    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp" />
            <size
                android:width="100dp"
                android:height="100dp" />
            <stroke
                android:width="10dp"
                android:color="@color/aircasting_white" />
        </shape>
    </item>

</layer-list>